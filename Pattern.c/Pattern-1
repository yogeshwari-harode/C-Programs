#include<stdio.h>
#include<conio.h>
int main()
{
    int n,i,j;
    printf("Enter any number : ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1; j<=n; j++)
        { 
          if(j>i-1)
           {
              printf("%d",j);
           }
          else
           { 
             printf(" ");
           }
        }
      printf("\n");

    }
    return 0;
}
/*
	12345678 
	 2345678
	  345678 
	   45678 
	    5678 
             678 
              78      
               8     
*/	       
	       
