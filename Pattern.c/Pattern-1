#include<stdio.h>
#include<conio.h>
int main()
{
    int n,i,j;
    printf("Enter any number : ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1; j<=n; j++)
        { 
          if(j>i-1)
           {
              printf("%d",j);
           }
          else
           { 
             printf(" ");
           }
        }
      printf("\n");

    }
    return 0;
}
/*
	1 2 3 4 5 6 7 8
	  2 3 4 5 6 7 8
	    3 4 5 6 7 8 
	      4 5 6 7 8
	        5 6 7 8 
            6 7 8 
              7 8
                8
*/	       
	       
