#include<stdio.h>
#include<conio.h>
int main()
{
	int n,i,count=0;
	
	//count is the variable used for how many numbers devide the given number
	
	printf("Enter any number : ");
	scanf("%d",&n);
	
		for(i=1;i<=n;i++)
		{
			if(n%i==0)
			{
				count++;
			}
		}
		
	if(count==2)
	{
		printf("The given number is prime.");
	}
	else{
		printf("The given number is not prime.");
	}
		
	
	return 0;
}
