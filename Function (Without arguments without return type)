#include<stdio.h>
#include<conio.h>
int main()
{
    void add(void);    //function prototype or fuction declaration
    add();             //function call
}

void add()
{
                        //function definition
     int a,b,c;
     printf("Enter the value of a,b : ");
     scanf("%d%d",&a,&b);
     c=a+b;
     printf("Sum = %d",c);
}
