#include<stdio.h>
#include<conio.h>
int main()
{
    int n,i=1,sum=0;
    
    printf("Enter the value of n : ");
    scanf("%d",&n);
    
    do
    {
        printf("%d\n",i);
        sum=sum+i;
        i=i+2;
    } while (i<=n);  
    printf("Sum of Odd numbers from 1-%d : %d",n,sum);
    
return 0;
}
