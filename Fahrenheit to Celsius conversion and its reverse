#include<stdio.h>
#include<conio.h>
int main()
{
    int Celsius,Fahrenheit;
    printf("Enter celsius : ");
    scanf("%d C",&Celsius);
    Fahrenheit = Celsius* 1.8 + 32;
    printf("%d F",Fahrenheit);
    
    printf(" \nNow see its reverse ");
    Celsius = (Fahrenheit - 32) / 1.8;
    printf("%d C",Celsius);
    return 0;
}
